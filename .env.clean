# CLEAN ENVIRONMENT - NO SECRETS
# Copy this to .env and add your NEW secrets

# Server Configuration
PORT=5001
NODE_ENV=development

# JWT Configuration - GENERATE NEW SECRET
JWT_SECRET=REPLACE_WITH_NEW_256_BIT_SECRET
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000

# DeepSeek API Configuration - GET NEW KEY
DEEPSEEK_API_KEY=sk-NEW_KEY_HERE
REACT_APP_DEEPSEEK_API_KEY=sk-NEW_KEY_HERE

# Supabase Configuration - ROTATE ALL KEYS
REACT_APP_SUPABASE_URL=https://your-new-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=NEW_ANON_KEY_HERE
SUPABASE_SERVICE_ROLE_KEY=NEW_SERVICE_ROLE_KEY_HERE
SUPABASE_JWT_SECRET=NEW_JWT_SECRET_HERE

# Paystack Configuration
REACT_APP_PAYSTACK_PUBLIC_KEY=pk_test_NEW_KEY_HERE

# Site Configuration
SITE_URL=http://localhost:5002